<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.progaleria.fragments.inicio.InicioFragment"
        android:label="@string/title_inicio"
        tools:layout="@layout/fragment_inicio" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.progaleria.fragments.galeria.GaleriaFragment"
        android:label="@string/title_galeria"
        tools:layout="@layout/fragment_galeria" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.progaleria.fragments.ubicacion.vista.UbicacionFragment"
        android:label="@string/title_ubicacion"
        tools:layout="@layout/fragment_ubicacion" />
